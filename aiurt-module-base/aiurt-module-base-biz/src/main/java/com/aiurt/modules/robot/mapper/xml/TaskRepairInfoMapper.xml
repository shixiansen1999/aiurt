<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aiurt.modules.robot.mapper.TaskRepairInfoMapper">
    <select id="queryByTaskId" resultType="com.aiurt.modules.robot.entity.TaskRepairInfo">
        select * from task_repair_info
        <where>
            <if test="taskId != null and taskId != ''">
                task_id =#{taskId}
            </if>
        </where>
    </select>
</mapper>