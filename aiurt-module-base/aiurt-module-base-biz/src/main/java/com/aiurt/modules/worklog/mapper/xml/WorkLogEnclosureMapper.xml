<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aiurt.modules.worklog.mapper.WorkLogEnclosureMapper">

    <select id="query" resultType="String">
        select url from work_log_enclosure where parent_id = #{id} and del_flag = '0' and type = #{type}
    </select>

    <delete id="deleteByName">
        delete from work_log_enclosure where parent_id = #{id}
    </delete>

</mapper>