<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aiurt.modules.schedule.mapper.ScheduleHolidaysMapper">

    <select id="getListByMonth" resultType="com.aiurt.modules.schedule.entity.ScheduleHolidays">
        select
            *
        from
            schedule_holidays where DATE_FORMAT(date, '%Y-%m') = #{date}
    </select>
</mapper>
