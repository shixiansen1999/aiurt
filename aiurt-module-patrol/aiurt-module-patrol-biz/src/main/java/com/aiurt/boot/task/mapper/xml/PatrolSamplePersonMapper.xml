<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aiurt.boot.task.mapper.PatrolSamplePersonMapper">
    <select id="getSamplePersonList" resultType="com.aiurt.boot.task.entity.PatrolSamplePerson">
        select *
        from patrol_sample_person
        where task_device_code = #{patrolNumber}
        and user_id is not null
        and username is not null
    </select>
</mapper>