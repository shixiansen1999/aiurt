<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aiurt.modules.material.mapper.MaterialBaseMapper">
    <select id="selectByCode" resultType="com.aiurt.modules.material.entity.MaterialBase">
        select * from material_base where `code` = #{code} and del_flag = 0  limit 1
    </select>
</mapper>
