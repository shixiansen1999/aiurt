<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aiurt.boot.modules.device.mapper.DeviceTypeMapper">

    <select id="getTypeByCode" resultType="string">
        select name from device_type
        where del_flag=0
        and code=#{code}
        limit 1
    </select>

    <select id="getDeviceType" resultType="java.lang.Integer">
        select * from device_type
        where del_flag=0
        and code=#{code}
    </select>

    <select id="getDeviceTypeListBySystemCode" resultType="com.aiurt.boot.modules.device.entity.DeviceType">
        select * from device_type
        where del_flag=0
        and system_code=#{systemCode}
    </select>
</mapper>
