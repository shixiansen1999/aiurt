<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aiurt.boot.modules.secondLevelWarehouse.mapper.SparePartStockInfoMapper">

    <select id="queryList" resultType="com.aiurt.boot.modules.secondLevelWarehouse.entity.SparePartStockVO">
        SELECT
	        id AS id,
	        stock_code AS warehouseCode,
	        stock_name AS warehouseName,
	        description AS description
        FROM
	        cs_stock_spare_part
        WHERE
	        STATUS = 0
	        AND del_flag =0
    </select>
</mapper>
