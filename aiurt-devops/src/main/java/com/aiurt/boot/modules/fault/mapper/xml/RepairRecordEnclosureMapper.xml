<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aiurt.boot.modules.fault.mapper.RepairRecordEnclosureMapper">

    <select id="queryDetail" resultType="String">
        select url from repair_record_enclosure where repair_record_id = #{repairRecordId} and del_flag = '0' and type = #{type}
    </select>

    <delete id="deleteByName">
        delete from repair_record_enclosure where repair_record_id = #{id}
    </delete>

</mapper>
