<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aiurt.boot.modules.fault.mapper.KnowledgeBaseEnclosureMapper">

    <select id="selectByKnowledgeId" resultType="String">
        SELECT url FROM `knowledge_base_enclosure` where knowledge_base_id = #{id}
    </select>

    <delete id="deleteByName">
        delete from knowledge_base_enclosure where knowledge_base_id = #{id}
    </delete>
</mapper>
